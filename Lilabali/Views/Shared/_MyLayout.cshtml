<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Lilabali</title>
    <link href="~/Content/lilabali.css" rel="stylesheet" />
    <script src="
https://cdn.jsdelivr.net/npm/toastr@2.1.4/toastr.min.js
"></script>
    <link href="
https://cdn.jsdelivr.net/npm/toastr@2.1.4/build/toastr.min.css
" rel="stylesheet">
    <script src="~/Scripts/jquery-3.4.1.js"></script>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="container">
        <div class="card">
            <div class="card-header">
                <div class="row">
                    <div class="col-sm-3">
                        <h5>Lilabali</h5>
                    </div>
                    <div class="col-sm-9" style="text-align:right;">
                        <a class="btn btn-sm btn-dark" href="@Url.Action("Home","Lilabali")">Members</a>
                        <a class="btn btn-sm btn-dark" href="@Url.Action("Teams","Lilabali")">Teams</a>
                        <a class="btn btn-sm btn-dark" href="@Url.Action("Payment","Lilabali")">Payments</a>
                    </div>
                </div>
            </div>
            <div class="card-body">
                @RenderBody()
            </div>
           
        </div>
    </div>
    <footer class="bg-warning fixed-bottom" style="text-align:center;">
        <img src="~/images/LEADS rb.png" width="150" />
    </footer>




    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
